        <contexts>
            <context name="Orchestra" attribute="Normal Text" lineEndContext="#stay">
                <IncludeRules context="SharedRules" />
                <StringDetect attribute="Keyword" context="#stay" String="instr " beginRegion="Instr" />
                <StringDetect attribute="Keyword" context="#stay" String="endin" endRegion="Instr" />
                <StringDetect attribute="Region Marker" context="Region Marker" String="//BEGIN" beginRegion="Region" firstNonSpace="true" />
                <StringDetect attribute="Region Marker" context="Region Marker" String="//END" endRegion="Region" firstNonSpace="true" />
                <keyword attribute="Keyword" context="#stay" String="opcodes"/>
                <keyword attribute="Definition" context="#stay" String="orchestraHeaderStatements"/>
                <RegExpr attribute="Label" context="#stay" String="[a-zA-Z0-9_]+\:" firstNonSpace="true"/>
                <RegExpr attribute="pVariable" context="#stay" String="p[0-9]+" />
                <RegExpr attribute="iVariable" context="#stay" String="i[a-zA-Z0-9_]+" />
                <RegExpr attribute="kVariable" context="#stay" String="k[a-zA-Z0-9_]+" />
                <RegExpr attribute="aVariable" context="#stay" String="a[a-zA-Z0-9_]+" />
                <RegExpr attribute="gVariable" context="#stay" String="g[a-zA-Z0-9_]+" />
                <DetectIdentifier />
            </context>
            <context name="SharedRules" attribute="Normal Text" lineEndContext="#stay">
                <DetectSpaces />
                <DetectChar attribute="Comment" context="Commentar 1" char=";"/>
                <StringDetect attribute="Comment" context="Commentar 1" String="//"/>
                <Detect2Chars attribute="Comment" context="Commentar 2" char="/" char1="*" beginRegion="Comment"/>
                <StringDetect attribute="Include" context="#stay" String="#include " firstNonSpace="true"/>
                <RegExpr attribute="MacroDefinition" context="#stay" String="#define .*#" beginRegion="Macro" firstNonSpace="true"/>
                <DetectChar attribute="MacroDefinition" context="#stay" char="#" endRegion="Macro" />
                <RegExpr attribute="MacroExpansion" context="#stay" String="\$[a-zA-Z0-9_]+" />
                <Float attribute="Float" context="#stay">
                    <AnyChar String="fF" attribute="Float" context="#stay"/>
                </Float>
                <HlCOct attribute="Octal" context="#stay"/>
                <HlCHex attribute="Hex" context="#stay"/>
                <Int attribute="Decimal" context="#stay" />
                <HlCChar attribute="Char" context="#stay"/>
                <RegExpr attribute="CsdTag" context="#stay" String="&lt;.*&gt;" beginRegion="FileB" firstNonSpace="true" />
                <DetectChar attribute="String" context="String" char="&quot;"/>
                <AnyChar attribute="Symbol" context="#stay" String=":!%&amp;()+,-/.*&lt;=&gt;?[]|~^&#59;"/>
            </context>
            <context attribute="String" lineEndContext="#pop" name="String">
                <LineContinue attribute="String" context="#stay"/>
                <HlCStringChar attribute="String Char" context="#stay"/>
                <DetectChar attribute="String" context="#pop" char="&quot;"/>
            </context>
